<Window x:Class="Hemuppgift_WPFtemplate.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hemuppgift_WPFtemplate"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>

        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Frame x:Name="MainFrame"/>
        <Button x:Name="gridBtn" Content="Grid" FontSize="26" Click="NavigateToGrid"/>
        <Button x:Name="stkBtn" Content="StackPanel" FontSize="26" Grid.Column="1" Click="NavigateToStack"/>
        <Button x:Name="wrpBtn" Content="WrapPanel" FontSize="26" Grid.Row="1" Click="NavigateToWrap"/>
        <Button x:Name="dckBtn" Content="DockPanel" FontSize="26" Grid.Row="1" Grid.Column="1" Click="NavigateToDock"/>
    </Grid>
</Window>
