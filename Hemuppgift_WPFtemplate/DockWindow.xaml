<Window x:Class="Hemuppgift_WPFtemplate.DockWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hemuppgift_WPFtemplate"
        mc:Ignorable="d"
        Title="DockWindow" Height="450" Width="800">
       
            <DockPanel x:Name="mainDock">
        <Button DockPanel.Dock="Top" Content="Top" Height="20" Width="70" />
        <Button DockPanel.Dock="Bottom" Content="Bottom" Height="20" Width="70"/>
        <Button DockPanel.Dock="Left" Content="Left" Height="20" Width="70"/>
        <Button DockPanel.Dock="Right" Content="Right" Height="20" Width="70"/>
        <Button x:Name="centerBtn" Content="Center" Height="20" Width="70" Click="Button_Click"/>

       
    </DockPanel>

        
</Window>
